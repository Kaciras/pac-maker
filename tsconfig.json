{
	"references": [{
		"path": "./tsconfig.test.json",
	}],
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "lib",
		"sourceMap": true,
		"declaration": true,
		"useUnknownInCatchVariables": false
	}
