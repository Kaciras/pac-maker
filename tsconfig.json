{
	"include": ["src"],
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"lib": ["ESNext"],
		"strict": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"outDir": "lib",
		"sourceMap": true,
		"declaration": true,
		"useUnknownInCatchVariables": false
	}
}
