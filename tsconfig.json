{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"lib": ["ESNext"],
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"useUnknownInCatchVariables": false,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true
	}
}
